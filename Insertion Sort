def insertion_sort(list):
    for index in range (1,len(list)):
        value = list[index]
        i = index - 1
        while i>=0:
            if value < list[i]:
                list[i+1] = list[i]
                list[i]  = value
                i -= 1
            else:
                break
list = [1,-2,30,2,4,5,6,72,-8,9,10]
insertion_sort(list)
print(list)
